<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jason.listviewoverview"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/myAppTheme" >
        <activity
            android:name=".MainActivity"           
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>  
        <activity android:name="com.jason.listviewoverview.adapter.arrayadapter.ArrayListActivity" />
        <activity android:name="com.jason.listviewoverview.adapter.simpleAdapter.SimpleAdapterActivity" />
        <activity android:name="com.jason.listviewoverview.adapter.simpleAdapter.SimpleImageActivity" />      
        <activity android:name="com.jason.listviewoverview.adapter.readjson.JSONTextActivity" />
        <activity android:name="com.jason.listviewoverview.adapter.baseAdapter.BaseAdapterActivity" />
        <activity android:name="com.jason.listviewoverview.adapter.simpleAdapter.SimpleAdapterSpinner" />
        <activity android:name="com.jason.listviewoverview.pullload.XListViewActivity" />
     
    </application>

</manifest>
